<body>
    <div class="text-bg-light p-3 d-flex align-items-center">
        <img src="/logos/kainos_logo.png" alt="Kainos Logo" style="height: 80px;">
        <h1 class="ms-3 mb-0 flex-grow-1 text-end w-100">Open Job Roles</h1>
    </div>
    <div class="table-responsive">
        <table class="table table-dark table-hover">
            <thead>
                <tr>
                    <th>Role Name</th>
                    <th>Location</th>
                    <th>Capability</th>
                    <th>Band</th>
                    <th>Closing Date</th>
                </tr>
            </thead>
            <tbody>
                <% roles.forEach(function(role) { %>
                    <tr>
                        <td>
                            <%= role.roleName %>
                        </td>
                        <td>
                            <%= role.location %>
                        </td>
                        <td>
                            <%= role.capability.capabilityName %>
                        </td>
                        <td>
                            <%= role.band.bandName %>
                        </td>
                        <td>
                            <%= new Date(role.closingDate).toLocaleDateString('en-GB') %>
                        </td>
                    </tr>
                    <% }); %>
            </tbody>
        </table>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-FKyoEForCGlyvwx9Hj09JcYn3nv7wiPVlz7YYwJrWVcXK/BmnVDxM+D2scQbITxI"
        crossorigin="anonymous"></script>
</body>
